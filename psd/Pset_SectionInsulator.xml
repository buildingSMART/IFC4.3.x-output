<?xml version="1.0" ?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4X3_DEV_fc437a4"/>
  <Name>Pset_SectionInsulator</Name>
  <Definition>Properties applicable to the insulator type of discrete accessory, indicated that the insulator is a section insulator used in the overhead contact line system.</Definition>
  <Applicability/>
  <ApplicableClasses>
    <ClassName>IfcDiscreteAccessory/INSULATOR</ClassName>
    <ClassName>IfcDiscreteAccessoryType/INSULATOR</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcDiscreteAccessory/INSULATOR,IfcDiscreteAccessoryType/INSULATOR</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef>
      <Name>ACResistance</Name>
      <Definition>The resistance under AC.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcElectricResistanceMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>NumberOfWires</Name>
      <Definition>The number of wires used in the element.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcCountMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>IsArcSuppressing</Name>
      <Definition>Indicates whether the element has the ability to suppress an arc.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcBoolean"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>TensileStrength</Name>
      <Definition>Indicates the ability to withstand breakage apart under applied force.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcForceMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
