<?xml version="1.0" ?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" templatetype="PSET_MATERIALDRIVEN" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4X3_DEV_0c312d3"/>
  <Name>Pset_MaterialCommon</Name>
  <Definition>A set of general material properties.</Definition>
  <Applicability/>
  <ApplicableClasses>
    <ClassName>IfcMaterial</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcMaterial</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef>
      <Name>MolecularWeight</Name>
      <Definition>Molecular weight of material (typically gas).</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcMolecularWeightMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>Porosity</Name>
      <Definition>The void fraction of the total volume occupied by material (Vbr - Vnet)/Vbr.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcNormalisedRatioMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>MassDensity</Name>
      <Definition>Material mass density.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcMassDensityMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
