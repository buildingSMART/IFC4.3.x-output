      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcTimeSeries"/>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>OverallEfficiency</Name>
      <Definition>The pump and motor overall operational efficiency.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcTimeSeries"/>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>PressureRise</Name>
      <Definition>The developed pressure.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcTimeSeries"/>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>RotationSpeed</Name>
      <Definition>Pump rotational speed.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcTimeSeries"/>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>Flowrate</Name>
      <Definition>The actual operational fluid flowrate.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcTimeSeries"/>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>Power</Name>
      <Definition>The actual power consumption of the pump.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcTimeSeries"/>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
