    <ClassName>IfcPump</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcPump</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef>
      <Name>MechanicalEfficiency</Name>
