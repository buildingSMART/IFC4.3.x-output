<?xml version="1.0" ?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4X3_DEV_a9ccaec"/>
  <Name>Pset_CommunicationsApplianceTypeTransportEquipment</Name>
  <Definition>Properties common to a transport equipment. This property set is applied to a type or occurrence of _IfcCommunicationsAppliance_ with predefined type TRANSPORTEQUIPMENT.</Definition>
  <Applicability/>
  <ApplicableClasses>
    <ClassName>IfcCommunicationsAppliance/TRANSPORTEQUIPMENT</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcCommunicationsAppliance/TRANSPORTEQUIPMENT</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef>
      <Name>IsUpgradable</Name>
      <Definition>Indicates whether the transport equipment can be upgraded or not.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcBoolean"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>ElectricalCrossCapacity</Name>
      <Definition>Indicates the electrical cross capacity of the transport equipment.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>NumberOfSlots</Name>
      <Definition>Indicates the number of  slots.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcCountMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>TransportEquipmentType</Name>
      <Definition>Indicates the type of transport equipment.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_TransportEquipmentType">
            <EnumItem>MPLS_TP</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>OTN</EnumItem>
            <EnumItem>PDH</EnumItem>
            <EnumItem>SDH</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>TransportEquipmentAssemblyType</Name>
      <Definition>Indicates the type of transport equipment assembly.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_TransportEquipmentAssemblyType">
            <EnumItem>FIXEDCONFIGURATION</EnumItem>
            <EnumItem>MODULARCONFIGURATION</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
