      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcMassFlowRateMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>NitrogenOxidesEmissions</Name>
      <Definition>Rate of emission of nitrogen oxides</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcMassFlowRateMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>ParticulateMatterEmissions</Name>
      <Definition>Rate of emission of particulate matter</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcMassFlowRateMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>NoiseEmissions</Name>
      <Definition>Level of sound emission</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcSoundPowerLevelMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
