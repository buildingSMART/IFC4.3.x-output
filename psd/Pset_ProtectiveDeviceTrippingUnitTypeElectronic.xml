    <PropertyDef>
      <Name>N_Protection_100</Name>
      <Definition/>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcBoolean"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>N_Protection_Select</Name>
      <Definition/>
