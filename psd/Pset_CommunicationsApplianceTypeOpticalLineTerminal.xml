<?xml version="1.0" ?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4X3_DEV_897e1dd"/>
  <Name>Pset_CommunicationsApplianceTypeOpticalLineTerminal</Name>
  <Definition>Properties common to a optical line terminal. This property set is applied to a type or occurrence of IfcCommunicationsAppliance with predefined type OPTICALLINETERMINAL.</Definition>
  <Applicability/>
  <ApplicableClasses>
    <ClassName>IfcCommunicationsAppliance/OPTICALLINETERMINAL</ClassName>
    <ClassName>IfcCommunicationsApplianceType/OPTICALLINETERMINAL</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcCommunicationsAppliance/OPTICALLINETERMINAL,IfcCommunicationsApplianceType/OPTICALLINETERMINAL</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef>
      <Name>NumberOfSlots</Name>
      <Definition>Indicates the number of  slots.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcCountMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>NumberOfInterfaces</Name>
      <Definition>Indicates the types of interfaces and their number in the device.</Definition>
      <PropertyType>
        <TypePropertyTableValue>
          <Expression/>
          <DefiningValue>
            <DataType type="IfcLabel"/>
          </DefiningValue>
          <DefinedValue>
            <DataType type="IfcInteger"/>
          </DefinedValue>
        </TypePropertyTableValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
