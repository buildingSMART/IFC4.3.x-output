            <EnumItem>UNSET</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>PictogramEscapeDirection</Name>
      <Definition>The direction of escape pictogram.</Definition>
