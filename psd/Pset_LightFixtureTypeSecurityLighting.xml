      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_AddressabilityType">
            <EnumItem>IMPLEMENTED</EnumItem>
            <EnumItem>NOTIMPLEMENTED</EnumItem>
