      <Name>SecurityLightingType</Name>
      <Definition>The type of security lighting.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_LightFixtureSecurityLightingType">
            <EnumItem>BLUEILLUMINATION</EnumItem>
            <EnumItem>EMERGENCYEXITLIGHT</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>SAFETYLIGHT</EnumItem>
            <EnumItem>UNSET</EnumItem>
            <EnumItem>WARNINGLIGHT</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>FixtureHeight</Name>
      <Definition>The height of the fixture, such as the text height of an exit sign.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>SelfTestFunction</Name>
      <Definition>The type of self test function.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_SelfTestType">
            <EnumItem>CENTRAL</EnumItem>
            <EnumItem>LOCAL</EnumItem>
            <EnumItem>NONE</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>BackupSupplySystem</Name>
