            <EnumItem>SUPPORT_FITTING</EnumItem>
            <EnumItem>SUSPENSION_FITTING</EnumItem>
            <EnumItem>TENSIONING_FITTING</EnumItem>
            <EnumItem>TERMINATION_FITTING</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
