      <Name>ConditionTargetPerformance</Name>
      <Definition>Target condition of the asset</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_AssetRating">
            <EnumItem>HIGH</EnumItem>
            <EnumItem>LOW</EnumItem>
            <EnumItem>MODERATE</EnumItem>
