<?xml version="1.0" ?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4X3_DEV_559dffc"/>
  <Name>Pset_DistributionPortPHistoryCable</Name>
  <Definition>Log of electrical activity attached to an instance of IfcPerformanceHistory having an assigned IfcDistributionPort of type CABLE.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>ApparentPower</Name>
      <Definition>Apparent power.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPowerMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>Current</Name>
      <Definition>Log of electrical current.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcElectricCurrentMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>DataReceived</Name>
      <Definition>For data ports, captures log of data received.  The LIST at IfcTimeSeriesValue.Values may split out data according to Pset_DistributionPortTypeCable.Protocols.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcText"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>DataTransmitted</Name>
      <Definition>For data ports, captures log of data transmitted.  The LIST at IfcTimeSeriesValue.Values may split out data according to Pset_DistributionPortTypeCable.Protocols.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcText"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>PowerFactor</Name>
      <Definition>Power factor.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcRatioMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>ReactivePower</Name>
      <Definition>Reactive power.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPowerMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>RealPower</Name>
      <Definition>Real power.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPowerMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>Voltage</Name>
      <Definition>Log of electrical voltage.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcElectricVoltageMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
