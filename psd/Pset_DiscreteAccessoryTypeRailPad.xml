  <PropertyDefs>
    <PropertyDef>
      <Name>RailPadStiffness</Name>
      <Definition>Indicates the stiffness of a rail pad.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_RailPadStiffness">
            <EnumItem>MEDIUM</EnumItem>
            <EnumItem>SOFT</EnumItem>
            <EnumItem>STIFF</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
