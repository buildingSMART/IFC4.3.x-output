<?xml version="1.0" ?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4X3_DEV_a47f52d"/>
  <Name>Pset_TankTypeExpansion</Name>
  <Definition>Common attributes of an expansion type tank.</Definition>
  <Applicability/>
  <ApplicableClasses>
    <ClassName>IfcTank/EXPANSION</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcTank/EXPANSION</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef>
      <Name>ChargePressure</Name>
      <Definition>Nominal or design operating pressure of the tank.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPressureMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>PressureRegulatorSetting</Name>
      <Definition>Pressure that is automatically maintained in the tank.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPressureMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>ReliefValveSetting</Name>
      <Definition>Pressure at which the relief valve activates.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPressureMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
