<?xml version="1.0" ?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4X3_DEV_62e1318"/>
  <Name>Pset_SensorTypeSnowSensor</Name>
  <Definition>Properties that are applicable for IfcSensor with predefined type SNOWDEPTHSENSOR.</Definition>
  <Applicability/>
  <ApplicableClasses>
    <ClassName>IfcSensor/SNOWDEPTHSENSOR</ClassName>
    <ClassName>IfcSensorType/SNOWDEPTHSENSOR</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcSensor/SNOWDEPTHSENSOR,IfcSensorType/SNOWDEPTHSENSOR</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef>
      <Name>MarginOfError</Name>
      <Definition>Indicates the margin of error of the measurement.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcRatioMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>SamplingFrequency</Name>
      <Definition>Indicates the sampling frequency of the device.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcFrequencyMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>DataCollectionType</Name>
      <Definition>Indicates the type or manner of data collection.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_DataCollectionType">
            <EnumItem>AUTOMATICANDCONTINUOUS</EnumItem>
            <EnumItem>MANUALANDSINGLE</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>ImageResolution</Name>
      <Definition>Indicates the image resolution of snow depth meter.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>ImageShootingMode</Name>
      <Definition>Indicates the type or manner of snow depth meter image shooting.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_ImageShootingMode">
            <EnumItem>AUTOMATIC</EnumItem>
            <EnumItem>MANUAL</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>SerialInterfaceType</Name>
      <Definition>Indicates the type of serial interface used by the device.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_SerialInterfaceType">
            <EnumItem>RS_232</EnumItem>
            <EnumItem>RS_422</EnumItem>
            <EnumItem>RS_485</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>LengthMeasureResolution</Name>
      <Definition>Indicates the resolution for length measure of the device.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLengthMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>SnowSensorMeasureRange</Name>
      <Definition>Indicates the measuring range of snow depth meter.</Definition>
      <PropertyType>
        <TypePropertyBoundedValue>
          <DataType type="IfcLengthMeasure"/>
        </TypePropertyBoundedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>SnowSensorType</Name>
      <Definition>Indicates the type of snow depth meter.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_SnowSensorType">
            <EnumItem>LASERIRRADIATION</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
