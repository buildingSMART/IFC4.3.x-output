<?xml version="1.0" ?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4X3_DEV_e62bdcd"/>
  <Name>Pset_AudioVisualApplianceTypeDisplay</Name>
  <Definition>An audio-visual display is a device that renders video from a screen.  HISTORY: Added in IFC4.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>AudioMode</Name>
      <Definition>Indicates audio sound modes and corresponding labels, if applicable.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>Brightness</Name>
      <Definition>Indicates the display brightness.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcIlluminanceMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>ContrastRatio</Name>
      <Definition>Indicates the display contrast ratio.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveRatioMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>DisplayHeight</Name>
      <Definition>Indicates the physical height of the screen (only the display surface).</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>DisplayType</Name>
      <Definition>Indicates the type of display.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>DisplayWidth</Name>
      <Definition>Indicates the physical width of the screen (only the display surface).</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>NominalSize</Name>
      <Definition>Indicates the diagonal screen size.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>RefreshRate</Name>
      <Definition>Indicates the display refresh frequency.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcFrequencyMeasure"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>TouchScreen</Name>
      <Definition>Indicates touchscreen support.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>VideoCaptionMode</Name>
      <Definition>Indicates video closed captioning modes.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>VideoResolutionHeight</Name>
      <Definition>Indicates the number of vertical pixels, e.g. 1080.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcInteger"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>VideoResolutionMode</Name>
      <Definition>Indicates video resolution modes.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>VideoResolutionWidth</Name>
      <Definition>Indicates the number of horizontal pixels, e.g. 1920.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcInteger"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>VideoScaleMode</Name>
      <Definition>Indicates video scaling modes.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
