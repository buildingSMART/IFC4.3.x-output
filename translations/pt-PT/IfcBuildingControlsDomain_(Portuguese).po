msgid "ActuatorApplication_NOTKNOWN"
msgstr "Não Conhecido"

msgid "ActuatorApplication_OTHER"
msgstr "Outro"

msgid "ActuatorApplication_SUNBLINDACTUATOR"
msgstr "Atuador de toldo, cortina ou persiana"

msgid "ActuatorApplication_UNSET"
msgstr "Não definido"

msgid "ActuatorApplication_VALVEPOSITIONER"
msgstr "Posicionador de Válvula"

msgid "ActuatorStatus"
msgstr "Estado do Atuador"

msgid "ActuatorStatus_DEFINITION"
msgstr "[[Status]] do elemento, predominantemente usado em projetos de renovação ou requalificação. O estatuto pode ser atribuído como: 'Novo' - elemento projetado como uma nova adição, 'Existente' - o elemento existe e permanece, 'Demolir' - o elemento existia, mas será demolido, 'Temporário' - o elemento existirá apenas temporariamente (como uma estrutura de apoio temporária)."

msgid "ActuatorStatus_DEMOLISH"
msgstr "'Demolir' - o elemento existe, mas vai ser demolido"

msgid "ActuatorStatus_EXISTING"
msgstr "'Existente' - o elemento existe e permanece"

msgid "ActuatorStatus_NEW"
msgstr "O estado pode ser atribuído como 'Novo' - elemento concebido como uma nova adição"

msgid "ActuatorStatus_NOTKNOWN"
msgstr "Desconhecido"

msgid "ActuatorStatus_OTHER"
msgstr "Outros"

msgid "ActuatorStatus_TEMPORARY"
msgstr "'Temporário' - o elemento só existirá temporariamente (como uma estrutura de suporte temporária)"

msgid "ActuatorStatus_UNSET"
msgstr "Não definido"

msgid "FailPosition"
msgstr "Posição de Falha"

msgid "FailPosition_DEFINITION"
msgstr "Especifica a posição de segurança requerida do atuador."

msgid "FailPosition_FAILCLOSED"
msgstr "Erro: Fechado"

msgid "FailPosition_FAILOPEN"
msgstr "Erro: Aberto"

