msgid "InsulationStandardClass_CLASSIIIAPPLIANCE"
msgstr "Aparelho de Classe III"

msgid "InsulationStandardClass_NOTKNOWN"
msgstr "Desconhecido"

msgid "InsulationStandardClass_OTHER"
msgstr "assim como outros fatores"

msgid "InsulationStandardClass_UNSET"
msgstr "Não definido"

msgid "IP_Code"
msgstr "Código de Grau de Proteção (IP)"

msgid "IP_Code_DEFINITION"
msgstr "O Código IP, a Etiqueta de Proteção Internacional, IEC 60529), classifica e classifica o grau de proteção fornecido contra a intrusão."

