                <ConceptTemplate applicableEntity="IfcSpace" applicableSchema="IFC4X3" name="Space Boundaries" status="sample" uuid="dc386523-b846-5f12-84c4-4ec2d4109f4a" isPartial="false">
                    <Definitions>
