                                                        <AttributeRule AttributeName="UnitType" RuleID="DerivedUnitType">
                                                            <EntityRules>
                                                                <EntityRule EntityName="IfcDerivedUnitEnum" />
                                                            </EntityRules>
                                                        </AttributeRule>
