                                                        <AttributeRule AttributeName="RepresentationType">
                                                            <EntityRules>
                                                                <EntityRule EntityName="IfcLabel" />
                                                            </EntityRules>
                                                        </AttributeRule>
                                                    </AttributeRules>
                                                </EntityRule>
                                            </EntityRules>
                                        </AttributeRule>
                                    </AttributeRules>
                                </EntityRule>
                            </EntityRules>
                        </AttributeRule>
                    </Rules>
                    <SubTemplates>
                        <ConceptTemplate applicableEntity="IfcProduct" applicableSchema="IFC4X3" name="Reference Topology" status="sample" uuid="864bb8a8-ce7d-5513-8e33-04df8e1db34c" isPartial="false">
                            <Definitions>
                                <Definition>
                                    <Body lang="en">&lt;p&gt;Structural activities may have a 'Reference' representation describing the distribution of a load.&lt;/p&gt;
&lt;p&gt;&lt;/p&gt;</Body>
                                </Definition>
                            </Definitions>
                            <Rules>
                                <AttributeRule AttributeName="Representation">
