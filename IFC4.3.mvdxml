                            <TemplateRules operator="and">
                                <TemplateRule Parameters="RelatingObject[Type]='IfcProject'" Description="Direct assignment to project, if the bridge is the outermost spatial container, and no site information is provided for bridge projects." />
                                <TemplateRule Parameters="RelatingObject[Type]='IfcSite'" Description="Assignment to site, if the bridge is the spatial container for the bridge project with site information." />
                                <TemplateRule Parameters="RelatingObject[Type]='IfcBridge'" Description="Assignment to another bridge as spatial container, e.g. if this bridge represents a bridge section." />
                            </TemplateRules>
                        </Concept>
                        <Concept name="Spatial Decomposition" override="false" status="sample" uuid="cfd26821-6112-5af1-a9d1-3c87a8f9dc65">
