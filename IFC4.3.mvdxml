                        <ConceptTemplate applicableEntity="IfcObjectDefinition" applicableSchema="IFC4X3" name="Object Nesting" status="sample" uuid="65a3d399-be6a-529b-a715-e253970764c0" isPartial="false">
                            <Definitions>
                                <Definition>
                                    <Body lang="en">&lt;p&gt;General object nesting indicates an ordered arrangement relationship.&lt;/p&gt;
&lt;p&gt;&lt;/p&gt;</Body>
                                </Definition>
                            </Definitions>
                            <Rules>
                                <AttributeRule AttributeName="IsNestedBy">
                                    <EntityRules>
