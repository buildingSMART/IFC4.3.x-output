                            <Template ref="fa6797d6-762f-5c0a-a85d-07cea7e50e7e" />
                        </Concept>
                        <Concept name="Element Nesting" override="false" status="sample" uuid="eeec865f-63d9-5df9-ab73-c47da52b17e5">
                            <Definitions>
                                <Definition>
                                    <Body lang="en">&lt;p&gt;A host element can nest connected components. This should be used when there is a specific position or form factor to attach specific elements.&lt;/p&gt;
&lt;blockquote&gt;
&lt;p&gt;EXAMPLE A sink has a pre-drilled hole to attach a faucet.&lt;/p&gt;
&lt;/blockquote&gt;</Body>
                                </Definition>
                            </Definitions>
                            <Template ref="2cef26b3-c687-5d78-bc38-9cbc293ffeb0" />
                        </Concept>
                        <Concept name="Body AdvancedSweptSolid Geometry" override="false" status="sample" uuid="ba02dcf7-5bb2-54bd-ab78-03421f3e7298">
                            <Template ref="042bbe89-d822-5f31-8609-80d4d5f27914" />
                        </Concept>
